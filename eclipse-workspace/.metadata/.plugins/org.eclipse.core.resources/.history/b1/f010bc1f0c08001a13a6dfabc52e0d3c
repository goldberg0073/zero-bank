package day_07_unarycoperators;

public class QuizInterview {
	
	
	
	public static int[] Solution(int N){

		int[] result = new int[N];

		int sum = 0;

		for(int i=0; i < N-1; i++) {

		result[i] =i;

		sum += i;

		}

		result[N-1] = -sum;

		return result;

		    }
	
	public static void main(String[] args) {
		
		
		//Solution(4);
		System.out.println(Solution(4).toString());
		
	}

}
