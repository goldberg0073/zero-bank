package Day03_variables;


public class Q {
	
	public static void main(String[] a) {

		
	
	char myChar=65;
	char myChar2='a';
	
	
	System.out.println(myChar);
	System.out.println(myChar2);
	
	
	 System.out.println(12%16);
	
	
	    	
	        	double number = 5.2;
	        	System.out.println(number++);
	        	System.out.println(number);
	        	System.out.println(++number);
	        	System.out.println(number);
	        }
	    }
	
	
	}
	}
