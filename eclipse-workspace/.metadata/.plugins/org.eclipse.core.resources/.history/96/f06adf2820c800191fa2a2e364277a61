package day10_IfStatement;

public class if_else_Statement {

public static void main(String[] args) {
	
	
	int score=99;
	
	if (score>=60) {
		System.out.println("passed");
	}
	else {
		System.out.println("failed");
	}
	
	if (score>=90) {
		System.out.println("good job");
	}
	else {
		System.out.println("not good nob");
	}
	
}




}
